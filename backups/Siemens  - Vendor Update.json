  "name": "Siemens  - Vendor Update",
  "nodes": [
    {
      "parameters": {},
      "name": "Start",
      "type": "n8n-nodes-base.start",
      "typeVersion": 1,
      "position": [
        -880,
        300
      ]
    },
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "siemens/update-vendor",
        "responseMode": "lastNode",
        "options": {}
      },
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        -660,
        300
      ],
      "webhookId": "78141e75-9374-475c-b6c2-cfb40d0a625a"
    },
    {
      "parameters": {
        "functionCode": "let vendors = $node[\"Webhook\"].json.body.vendors;\n\nlet available_vendors = $node[\"Check Vendor ID\"].json.data;\n\n\nlet update_data = [];\n\nfor (var key in vendors) {\n    if (vendors.hasOwnProperty(key)) {\n        var vendor = vendors[key];\n        \n        available_vendors.forEach((available_vendor,index)=>{\n\n            if(available_vendor.id == vendor.siemens_vendor_id){\n                let keys = [];\n                keys.push(vendor.siemens_vendor_id);\n\n                let data = {\n                    \"company_id\": vendor.sd_store_id\n                }\n\n                update_data.push({\n                    json: [{\n                        keys:keys,\n                        data:data,\n                    }]\n                });\n            }\n        });\n    }\n}\n\nreturn update_data;\n"
      },
      "name": "Prepare Data",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        280,
        120
      ]
    },
    {
      "parameters": {
        "authentication": "headerAuth",
        "requestMethod": "PATCH",
        "url": "=https://noirqa.shakedeal.com/items/sds_vendors",
        "jsonParameters": true,
        "options": {},
        "bodyParametersJson": "={{$node[\"Prepare Data\"].json[\"0\"]}}"
      },
      "name": "Update Vendor ID",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        500,
        120
      ],
      "credentials": {
        "httpHeaderAuth": {
          "id": "90",
          "name": "Simens Order Token"
        }
      }
    },
    {
      "parameters": {
        "functionCode": "let vendors = $node[\"Webhook\"].json.body.vendors;\n\nlet vendor_ids = [];\n\nfor (var key in vendors) {\n    if (vendors.hasOwnProperty(key)) {\n        var vendor = vendors[key];\n        vendor_ids.push(vendor.siemens_vendor_id);\n    }\n}\n\nitems[0].json.vendor_ids = vendor_ids;\n\nreturn items;"
      },
      "name": "Get Vendor IDs",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        -300,
