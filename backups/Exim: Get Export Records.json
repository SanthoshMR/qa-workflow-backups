        "functionCode": "// Code here will run only once, no matter how many input items there are.\n// More info and help: https://docs.n8n.io/nodes/n8n-nodes-base.function\n// Tip: You can use luxon for dates and $jmespath for querying JSON structures\n\nlet matchExpressions = [];\nlet dateLeft = new Date(items[0].json[\"Month Start (DD-MM-YYYY)\"].replace( /(\\d{2})-(\\d{2})-(\\d{4})/, \"$2/$1/$3\"));\nlet dateRight = new Date(items[0].json[\"Month End (DD-MM-YYYY)\"].replace( /(\\d{2})-(\\d{2})-(\\d{4})/, \"$2/$1/$3\"));\nlet searchTerm = items[0].json[\"Search Term\"].toString();\nlet monthRangeIdentifier = {\"identifier\":\"SEARCH_MONTH_RANGE\",\"alias\":\"MONTH\",\"clause\":\"MATCH\",\"expressionType\":300,\"fieldTerm\":\"EXP_DATE\",\"fieldValue\":[],\"fieldTermTypeSuffix\":\"\",\"fieldValueLeft\": dateLeft.toISOString(),\"fieldValueRight\": dateRight.toISOString()};\nmatchExpressions.push(monthRangeIdentifier);\nlet searchExpression;\nswitch (items[0].json[\"Search On\"]) {\n  case \"HS_CODE\":\n    let padLeft = \"00000000\";\n    let padRight = \"99999999\";\n    let hsnCodeLeft = (searchTerm + padLeft).substring(0, padLeft.length);\n    let hsnCodeRight = (searchTerm + padRight).substring(0, padRight.length);\n    searchExpression = {\"identifier\":\"SEARCH_HS_CODE\",\"alias\":\"HS_CODE\",\"clause\":\"MATCH\",\"expressionType\":103,\"fieldTerm\":\"HS_CODE\",\"fieldValueLeft\":+hsnCodeLeft,\"fieldValueRight\":+hsnCodeRight,\"fieldTermTypeSuffix\":\".number\",\"fieldValue\":searchTerm,\"advanceSearchType\":false};\n    break;\n  case \"SELLER\":\n    let sellerNames = searchTerm.split(\",\");\n    searchExpression = {\"identifier\":\"SEARCH_SELLER\",\"alias\":\"SELLER\",\"clause\":\"MATCH\",\"expressionType\":102,\"fieldTerm\":\"EXPORTER_NAME\",\"fieldValue\":sellerNames,\"fieldTermTypeSuffix\":\".keyword\",\"advanceSearchType\":false};\n    break;\n}\n\nmatchExpressions.push(searchExpression);\nconsole.log(matchExpressions);\n\nreturn [{ length: 100, start